    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@types/bun": "latest",
    "@types/eslint": "^8.56.10",
    "@types/graphql": "^14.5.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.39.2",
    "globals": "^15.4.0",
    "postcss": "^8.4.38",
